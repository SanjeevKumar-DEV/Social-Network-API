const router = require('express').Router();
const {
  getUsers,
  createUser,
  getSingleUser,
} = require('../../controllers/userContoller');

// /api/users
router.route('/').get(getUsers).post(createUser);

// /api/users/:userId
router.route('/:userId').get(getSingleUser)
// .put(updateUser).delete(deleteUser);

module.exports = router;
